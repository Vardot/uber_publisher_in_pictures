{
  "name": "drupal/uber_publisher_in_pictures",
  "description": "News stories told and displayed in a form of picture galleries.",
  "type": "drupal-module",
  "homepage": "https://www.drupal.org/project/uber_publisher_in_pictures",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Vardot",
      "homepage": "https://www.drupal.org/vardot",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://www.drupal.org/project/issues/uber_publisher_in_pictures",
    "source": "http://cgit.drupalcode.org/uber_publisher_in_pictures"
  },
  "repositories": {
    "drupal": {
